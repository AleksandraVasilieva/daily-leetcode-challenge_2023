class Solution:
    def runningSum(self,nums):
        answer = []
        for last in range(0, len(nums)):
            if len(answer) == 0:
                subanswer = nums[last]
                answer.append(subanswer)
            else:
                subanswer = nums[last] + answer[-1]
                answer.append(subanswer)
        return answer
